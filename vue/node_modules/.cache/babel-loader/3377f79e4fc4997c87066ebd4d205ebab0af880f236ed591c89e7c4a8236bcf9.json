r\n      this.$request.get(url).then(res => {\r\n        if (res.code === '200') {\r\n          this.recommend = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getRightData(url) {\r\n      this.$request.get(url).then(res => {\r\n        if (res.code === '200') {\r\n          this.rightData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getInformation() {\r\n      // 1. 获取推荐的那个资料\r\n      this.$request.get('/information/getRecommend').then(res => {\r\n        if (res.code === '200') {\r\n          this.fileRecommend = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n      // 2. 获取非推荐的8个资料\r\n      this.$request.get('/information/selectTop8').then(res => {\r\n        if (res.code === '200') {\r\n          this.leftData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    // loadRecommend() {\r\n    //   this.$request.get('/course/getRecommend').then(res => {\r\n    //     if (res.code === '200') {\r\n    //       this.recommend = res.data\r\n    //     } else {\r\n    //       this.$message.error(res.msg)\r\n    //     }\r\n    //   })\r\n    // },\r\n    // loadRightData() {\r\n    //   this.$request.get('/course/selectTop8').then(res => {\r\n    //     if (res.code === '200') {\r\n    //       this.rightData = res.data\r\n    //     } else {\r\n    //       this.$message.error(res.msg)\r\n    //     }\r\n    //   })\r\n    // }\r\n  }\r\n}\r\n</script>"],"mappings":"AAoEA;EAEAA,KAAA;IACA;MACAC,YAAA,GACAC,OAAA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MAAA,CACA;;MACAC,IAAA;MACAC,SAAA;MACAC,SAAA;MACAC;IACA;EACA;EACAC,QAAA;IACA;IACA;IACA,KAAAC,OAAA;IACA,KAAAC,cAAA;EACA;EACA;EACAC,OAAA;IACAC,UAAAR,IAAA;MACA,KAAAA,IAAA,GAAAA,IAAA;MACA,KAAAK,OAAA;IACA;IACAA,QAAA;MACA;MACA,qBAAAL,IAAA;QACA;QACA,KAAAS,YAAA;QACA;QACA,KAAAC,YAAA;MACA;QACA;QA